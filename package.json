{
  "name": "server",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "node ace build --production",
    "start": "node server.js",
    "dev": "node ace serve --watch",
    "format": "prettier --write \"**/*.{js,ts,tsx,css,json,md,yml,yaml}\"",
    "db:migrate": "node ace migration:run",
    "db:migrate:force": "node ace migration:run --force",
    "db:rollback": "node ace migration:rollback",
    "db:status": "node ace migration:status",
    "db:seed": "node ace db:seed",
    "db:seed:force": "node ace seed --force"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^5.3.7",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "adonis-preset-ts": "^2.1.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^3.4.1",
    "pino-pretty": "^5.1.3",
    "prettier": "^2.3.2",
    "typescript": "^4.4.2",
    "youch": "^2.2.2",
    "youch-terminal": "^1.1.1"
  },
  "dependencies": {
    "@adonisjs/auth": "^8.0.9",
    "@adonisjs/core": "^5.3.2",
    "@adonisjs/drive": "^2.0.6",
    "@adonisjs/lucid": "^15.0.3",
    "@adonisjs/redis": "^7.0.9",
    "@adonisjs/repl": "^3.1.6",
    "cloudinary": "^1.26.3",
    "luxon": "^1.28.0",
    "nanoid": "^3.1.25",
    "pg": "^8.7.1",
    "phc-bcrypt": "^1.0.7",
    "proxy-addr": "^2.0.7",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.19"
  }
